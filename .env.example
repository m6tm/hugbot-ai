# Supabase Configuration
DATABASE_URL=""
DIRECT_URL=""
PUBLIC_SUPABASE_URL=""
PUBLIC_SUPABASE_ANON_KEY=""
SUPABASE_SERVICE_ROLE_KEY=""

# JWT Configuration (for fallback) 
# Generated with the following command:
# openssl rand -base64 32
JWT_SECRET=""
JWT_EXPIRES_IN=7d

# Configuration de l'IA (Multi-provider via OpenAI SDK)
# Valeurs possibles pour AI_PROVIDER: huggingface, gemini, claude, openai
AI_PROVIDER=huggingface
AI_BASE_URL=

# Cle API Hugging Face par defaut (cote serveur uniquement)
# Obtenez votre cle sur: https://huggingface.co/settings/tokens
HUGGINGFACE_API_KEY=
HUGGINGFACE_BASE_URL=https://router.huggingface.co/v1

# OpenAI Configuration
OPENAI_API_KEY=
OPENAI_BASE_URL=https://api.openai.com/v1

# Google Gemini Configuration (via OpenAI compatible endpoint)
GEMINI_API_KEY=
GEMINI_BASE_URL=https://generativelanguage.googleapis.com/v1beta/openai/

# Anthropic Claude Configuration (necessite un proxy compatible OpenAI)
CLAUDE_API_KEY=
CLAUDE_BASE_URL=

# Inngest Configuration
# Nombre maximum de tentatives de reprise en cas d'erreur
INNGEST_MAX_RETRIES=5
